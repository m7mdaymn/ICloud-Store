{
  "buildCommand": "npm run build",
  "outputDirectory": "dist/storefront/browser",
  "env": {
    "NODE_ENV": "production",
    "NG_BUILD_PROD": "true"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://icloudstore.runasp.net/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
